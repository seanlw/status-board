{
  "name": "status-board",
  "description": "Flexable status board",
  "version": "2.0.0-alpha.2",
  "author": {
    "name": "Sean Watkins",
    "email": "seanlw@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanlw/status-board"
  },
  "license": "MIT",
  "productName": "Status Board",
  "appBundleId": "Status Board.app",
  "helperBundleId": "Status Board.app.helper",
  "main": "app/main.js",
  "scripts": {
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "electron": "npm run build && NODE_ENV=development electron app",
    "package": "npm run build && babel-node scripts/package.js --platform=all",
    "package:osx": "npm run build && babel-node scripts/package.js --platform=darwin",
    "package:win": "npm run build && babel-node scripts/package.js --platform=win32",
    "package:linux": "npm run build && babel-node scripts/package.js --platform=linux",
    "package:rpi": "npm run build && babel-node scripts/package.js --platform=rpi",
    "postinstall": "typings install",
    "start": "npm run electron"
  },
  "devDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@angular/upgrade": "^2.0.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.20",
    "babel": "^5.8.29",
    "c3": "^0.4.11",
    "core-js": "^2.4.1",
    "denodeify": "^1.2.1",
    "electron": "1.6.1",
    "electron-packager": "^8.6.0",
    "es6-shim": "^0.34.0",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "node-sass": "^3.8.0",
    "random-seed": "^0.3.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.2",
    "rxjs": "^5.0.0-beta.12",
    "sass-loader": "^4.0.0",
    "systemjs": "^0.19.27",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "async": "^1.3.0",
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.6.3",
    "google-spreadsheet": "^2.0.3",
    "zone.js": "0.7.2"
  }
}
